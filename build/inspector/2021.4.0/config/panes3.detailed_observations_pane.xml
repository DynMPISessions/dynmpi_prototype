<?xml version="1.0" encoding="utf-8"?>
<root>

  <column>
    <active type="string_vector">
      <!--Description-->
      <item>asdpdata_obs_desc</item>
      <!--Source-->
      <item>asdpdata_file_and_line</item>
      <!--Function-->
      <item>asdpdata_func_name</item>
      <!--Module-->
      <item>asdpdata_module_name</item>
      <!-- Memory leak size--> 
      <item>asdpdata_object_size</item>

      <!--The offset of access from heap allocated object -->
      <item>asdpdata_offset</item>
      
      <!--Difference mode column-->
      <item>asdpdata_diff_state_string</item>

      <!--Device attribute-->
      <item>asdpdata_device_attr</item>
      <!--Device value-->
      <item>asdpdata_device_value</item>
      <!--Device limit-->
      <item>asdpdata_device_limit</item>
    </active>

    <default_sortby type="string">asdpdata_obs_desc</default_sortby>

    <!--Map of available fields and rules to display it: map<field, rule>
    rule supports variables: analysis_type, diff_mode
    empty rule means that the field is always available-->
    
    <rules type="string_string_map">
      <!--Description-->
      <item key="asdpdata_obs_desc" value=""/>
      <!--Source-->
      <item key="asdpdata_file_and_line" value=""/>
      <!--Function-->
      <item key="asdpdata_func_name" value=""/>
      <!--Module-->
      <item key="asdpdata_module_name" value=""/>
      <!-- Memory leak size-->
      <item key="asdpdata_object_size" value="(collector == runmc) || (analysis_type == rawdata)"/>
           
      <!--The offset of access from heap allocated object -->
      <item key="asdpdata_offset" value="((collector == runmc) and (cknob_detect_invalid_accesses == true)) || (analysis_type == rawdata)"/>
      
      <!--Difference mode column-->
      <item key="asdpdata_diff_state_string" value="diff_mode == true"/>

      <!--Variable name-->
      <item key="asdpdata_best_object_variable" value=""/>

      <!--Device attribute-->
      <item key="asdpdata_device_attr" value=""/>
      <!--Device value-->
      <item key="asdpdata_device_value" value=""/>
      <!--Device limit-->
      <item key="asdpdata_device_limit" value=""/>
    </rules>

  </column>

</root>
