<?xml version='1.0' encoding='utf-8'?>
<root>

  <pane_splitters>
    <!--Pane splitter colors-->
    <clNormal type='ui_resource' kind='color' value='clThemeBk'/>
    <clHighlighted type='ui_resource' kind='color' value='clThemeHighlighBk'/>
  </pane_splitters>

  <colors>
    <!--Tab panel manager colors-->
    <tab_panel_mgr>
      <background type='ui_resource' kind='color' value='clThemeBk'/>
    </tab_panel_mgr>

    <!--Side panel colors-->
    <side_panel>
      <text type='ui_resource' kind='fixed_color' value='clThemeCaptionText'/>
    </side_panel>
  </colors>

  <pictures>
    <!--Tab panel manager pictures-->
    <tab_panel_mgr>
      <background type='ui_resource' kind='picture'>
        <image resource='client.xe.dat' name='tab-background'/>
      </background>

      <background-emberson type='ui_resource' kind='picture'>
        <image resource='client.xe.dat' name='tab-background-emberson'/>
      </background-emberson>

      <background-emberson14 type='ui_resource' kind='picture'>
        <image resource='client.xe.dat' name='tab-background-emberson14'/>
      </background-emberson14>

      <product_icon type='ui_resource' kind='picture' default_size='24 24' animation='false'>
        <image resource='client.xe.dat' name='product-icon'/>
      </product_icon>

      <collection_animation type='ui_resource' kind='picture' default_size='24 24' animation='true'>
        <image resource='client.xe.dat' name='collection-animation'/>
      </collection_animation>
    </tab_panel_mgr>

    <!--Side panel pictures-->
    <side_panel>

      <start>
        <active type='ui_resource' kind='picture' default_size='32 32' animation='false'>
          <image resource='client.dat' name='side-panel-start-32x32'/>
        </active>
        <disable type='ui_resource' kind='picture' default_size='32 32' animation='false'>
          <image resource='client.dat' name='side-panel-start-disabled-32x32'/>
        </disable>
      </start>

      <reinspect>
        <active type='ui_resource' kind='picture' default_size='32 32' animation='false'>
          <image resource='client.dat' name='side-panel-start-32x32'/>
        </active>
        <disable type='ui_resource' kind='picture' default_size='32 32' animation='false'>
          <image resource='client.dat' name='side-panel-start-disabled-32x32'/>
        </disable>
      </reinspect>

      <stop>
        <active type='ui_resource' kind='picture' default_size='16 16' animation='false'>
          <image resource='client.dat' name='side-panel-stop'/>
        </active>
        <disable type='ui_resource' kind='picture' default_size='16 16' animation='false'>
          <image resource='client.dat' name='side-panel-stop-disabled'/>
        </disable>
      </stop>

      <leak_baseline>
        <active type='ui_resource' kind='picture' default_size='16 16' animation='false'>
          <image resource='client.dat' name='side-panel-baseline-16x16'/>
        </active>
        <disable type='ui_resource' kind='picture' default_size='16 16' animation='false'>
          <image resource='client.dat' name='side-panel-baseline-disabled-16x16'/>
        </disable>
      </leak_baseline>

      <leak_snapshot>
        <active type='ui_resource' kind='picture' default_size='16 16' animation='false'>
          <image resource='client.dat' name='side-panel-snapshot-16x16'/>
        </active>
        <disable type='ui_resource' kind='picture' default_size='16 16' animation='false'>
          <image resource='client.dat' name='side-panel-snapshot-disabled-16x16'/>
        </disable>
      </leak_snapshot>

      <growth_baseline>
        <active type='ui_resource' kind='picture' default_size='16 16' animation='false'>
          <image resource='client.dat' name='side-panel-growth-baseline-16x16'/>
        </active>
        <disable type='ui_resource' kind='picture' default_size='16 16' animation='false'>
          <image resource='client.dat' name='side-panel-growth-baseline-disabled-16x16'/>
        </disable>
      </growth_baseline>

      <growth_snapshot>
        <active type='ui_resource' kind='picture' default_size='16 16' animation='false'>
          <image resource='client.dat' name='side-panel-growth-snapshot-16x16'/>
        </active>
        <disable type='ui_resource' kind='picture' default_size='16 16' animation='false'>
          <image resource='client.dat' name='side-panel-growth-snapshot-disabled-16x16'/>
        </disable>
      </growth_snapshot>

      <compare>
        <active type='ui_resource' kind='picture' default_size='32 32' animation='false'>
          <image resource='client.dat' name='side-panel-compare-32x32'/>
        </active>
        <disable type='ui_resource' kind='picture' default_size='32 32' animation='false'>
          <image resource='client.dat' name='side-panel-compare-disabled-32x32'/>
        </disable>
      </compare>

      <import>
        <active type='ui_resource' kind='picture' default_size='32 32' animation='false'>
          <image resource='client.dat' name='side-panel-import-32x32'/>
        </active>
        <disable type='ui_resource' kind='picture' default_size='32 32' animation='false'>
          <image resource='client.dat' name='side-panel-import-disabled-32x32'/>
        </disable>
      </import>

      <open>
        <active type='ui_resource' kind='picture' default_size='32 32' animation='false'>
          <image resource='client.dat' name='side-panel-start-32x32'/>
        </active>
        <disable type='ui_resource' kind='picture' default_size='32 32' animation='false'>
          <image resource='client.dat' name='side-panel-start-disabled-32x32'/>
        </disable>
      </open>

      <close>
        <active type='ui_resource' kind='picture' default_size='16 16' animation='false'>
          <image resource='client.dat' name='side-panel-cancel'/>
          <image resource='client.dat' name='side-panel-cancel-32x32'/>
        </active>
        <disable type='ui_resource' kind='picture' default_size='16 16' animation='false'>
          <image resource='client.dat' name='side-panel-cancel-disabled'/>
          <image resource='client.dat' name='side-panel-cancel-disabled-32x32'/>
        </disable>
      </close>

    </side_panel>

    <!-- Start menu icons -->
    <start_menu>
      <start_normal type='ui_resource' kind='picture' default_size='16 16' animation='false'>
        <image resource='client.xe.dat' name='start-normal'/>
      </start_normal>

      <start_w_debugger type='ui_resource' kind='picture' default_size='16 16' animation='false'>
        <image resource='client.xe.dat' name='start-debugger'/>
      </start_w_debugger>

      <start_w_debugger_paused type='ui_resource' kind='picture' default_size='16 16' animation='false'>
        <image resource='client.xe.dat' name='start-paused-debugger'/>
      </start_w_debugger_paused>
    </start_menu>

  </pictures>

</root>
