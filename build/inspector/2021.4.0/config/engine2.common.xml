<?xml version="1.0" encoding="utf-8"?>
<root>

  <constants>
    <diagostic_limit type="long">10000</diagostic_limit>

    <compiler_name_template type="string_vector">
      <item>name##major_sp#.#minor</item> <!--1st priority-->
      <item>name#-#major#.#minor</item>
      <item>name# #major#</item>
      <item>name#_#major#</item>
      <item>name#_#major#.#minor#.#patch#</item>
    </compiler_name_template>

    <template_splitter type="string">#</template_splitter>

    <compiler_folder>

      <windows type="string_vector">
        <item>Composer XE 2011 </item> <!--1st priority-->
        <item>Composer XE</item>
        <item>ComposerXE</item>
        <item>CompilerPro</item>
        <item>System Studio</item>
      </windows>

      <linux type="string_vector">
        <item>composer_xe_2011_</item> <!--1st priority-->
        <item>composer_xe</item>
        <item>composerxe</item>
        <item>compilerpro</item>
        <item>system_studio</item>
      </linux>

    </compiler_folder>

    <compiler_minimal_version type="int">12</compiler_minimal_version>

  <ssa_result_type_expr type="string">analysis_type == source_checker</ssa_result_type_expr>
  </constants>

</root>
